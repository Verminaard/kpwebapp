{
  "kind": "collectionType",
  "collectionName": "my_commentaries",
  "info": {
    "name": "MyCommentary"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Text": {
      "type": "richtext",
      "required": true,
      "maxLength": 300
    },
    "my_new": {
      "model": "my-news",
      "via": "my_commentaries"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "my_commentary"
    }
  }
}
