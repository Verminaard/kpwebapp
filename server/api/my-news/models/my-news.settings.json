{
  "kind": "collectionType",
  "collectionName": "my_news",
  "info": {
    "name": "MyNews"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Header": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "PreviewImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "Description": {
      "type": "string",
      "required": true
    },
    "Content": {
      "type": "richtext",
      "required": true,
      "minLength": 10
    },
    "my_commentaries": {
      "via": "my_new",
      "collection": "my-commentary"
    }
  }
}
